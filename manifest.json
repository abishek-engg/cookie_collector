{
    "manifest_version": 3,
    "action":{
        "default_popup": "index.html",
        "default_title": "moodle-assist"
    },
    "name": "moodl",
    "version": "1.0",
    "description": "A simple extension",
    "permissions": ["cookies","activeTab","downloads"],

    "host_permissions":["<all_urls>"],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["script.js"]
      }]
  }
  